{
    "manifest_version": 2,
  
    "name": "Word History",
    "description": "Find your word history wikipidia on the go!",
    "version": "1.0",
    "icons": {
      "128": "icon128.png",
      "48": "icon48.png",
      "16": "icon16.png"
    },
  
    "background": {
      "scripts": ["workPage.js"],
      "persistent": false
    },
  
    "permissions": [  
            "tabs",      
            "contextMenus"
          ]
  }